{
  "seed" : 10101010,
  "noise_table_size" : 50000000,
  "env_id" : "CartPole-v1",

  "server":
  {
    "address" : "localhost",
    "port" : 1025
  },

  "policy":
  {
    "type" : ["torch", "ffnn"],
    "init_std" : 0.05,
    "action_noise_std" : 0.00,
    "action_parser" : "sampling",
    "observation_buffer_length" : 1,
    "layers" : [32],
    "layer_functions" : ["tanh"],
    "layer_extras" : [null,null],
    "output_function" : "identity",
    "output_extras" : null,
    "normalize_observations" : false
  },

  "agent":
  {
    "type" : "torch dqn"
  },

  "gradient_optimizer":
  {
    "type" : "adam",
    "step_size": 0.05,
    "beta1": 0.9,
    "beta2": 0.999,
    "epsilon": 1e-8
  },

  "policy_optimizer":
  {
    "type" : "dqn",
    "gamma" : 0.99
    "noise_std" : 0.05,
    "obs_stats_update_prob" : 0.01,
    "reward_jiggle" : [-1e-4, 1e-4],
    "returns_per_update" : 300,
    "cost_function" : "fitness",
    "antithetic_sampling" : true,
    "vbn_buffer_size" : 1000,
    "reward_normalization_function" : "centered ranks"
  },

  "experience_replay":
  {
    "size": 100000,
    "initial_frames": 10000
  }
}